        <script>

    let numbers = [23,11,42,23,12,34,19];
//find is return velue 
    const find = numbers.find(x => x%2===0);

//findIndex is return index number of value
    const findIndex = numbers.findIndex(x => x%2===0);
    console.log(find);
    console.log(findIndex);

//now i used a object
